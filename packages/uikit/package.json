{
  "name": "@pancake-taro-toolkit/uikit",
  "version": "1.0.0",
  "description": "UI KIT for taro",
  "author": "opar-xu <opar.xu@binance.com>",
  "homepage": "",
  "license": "ISC",
  "module": "dist/esm/index.js",
  "main": "dist/lib/index.js",
  "source": "src/index.ts",
  "types": "./dist/types/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "lib",
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "yarn run dev:lib",
    "dev:lib": "tsc --project ./tsconfig.build.json --watch --incremental",
    "build": "npm-run-all build:*",
    "build:cjs": "rm -rf ./dist && tsc -p tsconfig.build.json -m commonjs --outDir dist/lib --declaration false",
    "build:es": "tsc -p tsconfig.build.json -m esNext --outDir dist/esm --declarationDir dist/types"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/styled-system": "^5.1.14",
    "npm-run-all": "^4.1.5",
    "styled-system": "^5.1.5"
  },
  "dependencies": {
    "@tarojs/components": "^3.3.19",
    "@tarojs/react": "^3.3.19",
    "@tarojs/taro": "^3.3.19",
    "lodash": "^4.17.21",
    "obj-flatten": "^2.0.6",
    "object-hash": "^2.2.0",
    "react-dom": "^17.0.2",
    "stylis": "^4.0.13"
  }
}
