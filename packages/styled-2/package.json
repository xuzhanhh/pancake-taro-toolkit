{
  "name": "@pancakeswap/mp-styled-2",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "module": "dist/esm/index.js",
  "main": "dist/lib/index.js",
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "linaria": {
    "tags": {
      "styled": "./processors/styled.js"
    }
  },
  "files": [
    "lib",
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "yarn run dev:lib",
    "dev:lib": "tsc --project ./tsconfig.build.json --watch --incremental",
    "build": "npm-run-all build:*",
    "build:cjs": "rm -rf ./dist/lib && tsc -p tsconfig.build.json -m commonjs --outDir dist/lib --declaration false",
    "build:es": "rm -rf ./dist/esm && rm -rf ./dist/types && tsc -p tsconfig.build.json -m esNext --outDir dist/esm --declarationDir dist/types",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:write": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "keywords": [],
  "author": "zane-x",
  "dependencies": {
    "@babel/preset-typescript": "^7.17.12",
    "@babel/types": "^7.18.4",
    "@emotion/is-prop-valid": "^1.1.3",
    "@types/babel__core": "^7.1.19",
    "@types/node": "^18.0.0",
    "esbuild": "^0.14.47",
    "react": "^17.0.2",
    "react-style-object-to-css": "^1.1.2"
  },
  "devDependencies": {
    "@linaria/core": "^3.0.0-beta.22",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.6.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2"
  }
}
